{
  "name": "config",
  "description": "config",
  "version": "4.0.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "private": true,
  "license": "BSD-3-Clause",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src",
    "!build/src/**/*.map"
  ],
  "jest": {
    "preset": "ts-jest",
    "testMatch": ["<rootDir>/test/**/*.ts"],
    "collectCoverage": true,
    "collectCoverageFrom": ["<rootDir>/src/**/*.ts"]
  },
  "dependencies": {
    "confidence": "4.0.2",
    "strip-json-comments": "3.0.1"
  },
  "devDependencies": {
    "@types/confidence": "1.4.30",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.4",
    "gts": "1.1.0",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "typescript": "3.6.2"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "posttest": "npm run check",
    "prepare": "npm run compile",
    "test": "jest",
    "test:watch": "npm test -- --watchAll",
    "watch": "tsc --watch"
  }
}
